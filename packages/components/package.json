{
  "name": "@antdv-plugins/core",
  "type": "module",
  "version": "0.0.1",
  "description": "this is antdv extra",
  "author": "",
  "license": "MIT",
  "keywords": [
    "ant-design-vue",
    "vue",
    "vue-components",
    "vue-plugins",
    "antdv"
  ],
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./es/*": "./es/*",
    "./lib/*": "./lib/*",
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "es",
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "build:dist": "vite build --config vite-bundle.config.ts"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.0",
    "@iconify/vue": "^4.1.1",
    "@vueuse/core": "^10.4.1",
    "ant-design-vue": "^4.0.3",
    "antdv-style": "0.0.1-beta.2",
    "vue": "^3.3.4"
  }
}
